@article{Loback2016,
abstract = {Throughout the brain, local circuits encode information using large populations of neurons, yet the structure of this population code remains poorly understood. Previous studies applying the maximum entropy principle to retinal multi-electrode array data have shown that the joint probability distribution of population responses is proliferated by local maxima, which represents a form of clustering of visual stimuli into a discrete set of categories. However, these studies focused on responses to highly repeated stimuli. Another recent study using a hidden Markov model approach likewise found that ganglion cell population activity partitioned into noise-robust clusters. Here, we investigate the relationship between the clusters found using these two fundamentally different modeling approaches. We first demonstrate that local maxima in the probability landscape result from the repeat structure of the stimulus ensemble, and are absent in the more realistic, non-repeated stimulus regime. Nonetheless, we show that structure is still present in the non-repeated stimulus regime in the form of what we call "soft local maxima," which are local maxima within the subspace of all neural activity states that are restricted to have the same spike count. Using a novel numerical approach, we show that soft local maxima are organized into a reliable sequence across the different spike count levels, which altogether has the geometry of a "ridge." These ridges closely corresponded with the clusters formulated by the hidden Markov model, and moreover with neuron "communities," in the sense defined in computer science. These findings suggest a unified description of the retinal population code across different stimulus repeat statistics, and suggest that ridges or communities may be an important feature of the output population activity extracted by visual processing areas downstream of the retina.},
archivePrefix = {arXiv},
arxivId = {1610.06886},
author = {Loback, Adrianna R. and Prentice, Jason S. and Ioffe, Mark L. and Berry, Michael J.},
doi = {10.5061/dryad.1f1rc.Funding},
eprint = {1610.06886},
file = {:home/nahuel/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Loback et al. - 2016 - Noise-robust modes of the retinal population code geometrically correspond with ridges(2).pdf:pdf},
pages = {1--34},
title = {{Noise-robust modes of the retinal population code geometrically correspond with "ridges"}},
url = {http://arxiv.org/abs/1610.06886},
year = {2016}
}
