file = {:home/nahuel/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Gao, Barzel, Barab{\'{a}}si - 2016 - Universal resilience patterns in complex networks(2).pdf:pdf},
isbn = {0008-5472 (Print){\$}\backslash{\$}r0008-5472 (Linking)},
issn = {14764687},
journal = {Nature},
