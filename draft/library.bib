keywords = {Betweenness centrality,Centroid,Increasing tree,Random tree,Simply generated tree,Subcritical graph class},
number = {96236},
pages = {33--52},
publisher = {Elsevier B.V.},
title = {{On the distribution of betweenness centrality in random trees}},
url = {http://dx.doi.org/10.1016/j.tcs.2016.12.023},
volume = {699},
year = {2017}
}
