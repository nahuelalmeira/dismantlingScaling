}
@misc{Belardo2014,
abstract = {Let $\Gamma$=(G,$\sigma$) be a connected signed graph, where G is the underlying simple graph and $\sigma$:E(G)→{\{}1,-1{\}} is the sign function on the edges of G. Let L($\Gamma$)=D(G)-A($\Gamma$), be the Laplacian of $\Gamma$ and $\lambda$1≥$\lambda$2≥{\^{a}}̄≥ $\lambda$n≥0 be its eigenvalues. It is well-known that a signed graph $\Gamma$ is balanced if and only if $\lambda$n($\Gamma$)=0. Here we show that, if $\Gamma$ is unbalanced, then $\lambda$n estimates how much $\Gamma$ is far from being balanced. In particular, let $\nu$($\Gamma$) (resp. {\"{I}}$\mu$($\Gamma$)) be the frustration number (resp. frustration index), that is the minimum number of vertices (resp. edges) to be deleted such that the signed graph is balanced. Then we prove that$\lambda$n($\Gamma$) ≤$\nu$($\Gamma$)≤{\"{I}}$\mu$($\Gamma$). Further we analyze the case when $\lambda$n($\Gamma$)=$\nu$($\Gamma$). In the latter setting, we identify the structure of the underlying graph G and we give an algebraic condition for L($\Gamma$) which leads to the above equality. {\textcopyright} 2014 Elsevier Inc.},
author = {Belardo, Francesco},
booktitle = {Linear Algebra and Its Applications},
doi = {10.1016/j.laa.2014.01.001},
