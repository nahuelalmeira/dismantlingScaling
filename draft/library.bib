issn = {1476-4687},
journal = {Nature},
keywords = {*Algorithms,*Models, Biological,*Probability,Biosynthetic Pathways,Food Chain,Gene Regulatory Networks,Metabolic Networks and Pathways,Protein Binding,Sensitivity and Specificity,Social Behavior},
number = {7191},
pages = {98--101},
pmid = {18451861},
title = {{Hierarchical structure and the prediction of missing links in networks}},
