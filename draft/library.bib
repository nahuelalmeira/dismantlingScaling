@article{Sun2018,
abstract = {The network dismantling problem is one of the most fundamental problems in network science. It aims to identify the minimum number of nodes, such that after their removal the network is broken into many disconnected pieces with a sub-extensive size. However, the identification of the minimum removed nodes belongs to the class of nondeterministic polynomial problems. Although many heuristic algorithms have been proposed to identify the removed nodes, the smallest dismantling set remains unknown. Therefore, the determination of a good lower bound of dismantling sets is of great significance to evaluating the performances of heuristic algorithms. The minimum number of deleted nodes to dismantle a network is strictly no smaller than that to dismantle its any subnetwork in nature. Any lower bound of a subnetwork is indeed a lower bound of the original network. Utilizing the heterogeneous degree distribution and 2-core properties, we find that with previous removal of some appropriate nodes, the lower bound obtained on the basis of the subnetwork is counterintuitively significantly better than the one obtained directly on the original network, especially for the real-world networks.
The study of the network dismantlement has attracted much attention of researchers because of its wide practical applications. However, the network dismantling problem is a nondeterministic polynomial (NP) problem and usually approximately solved by heuristic algorithms. The main problem of such heuristic algorithms is that they have no guarantee of their performances. Here, we study the lower bound of the minimum number of deleted nodes to dismantle a network. In particular, we first demonstrate that the lower bound of decycling sets solely based on tree structure is satisfied in the dismantled network. Moreover, the lower bound can be remarkably improved via specific subnetworks due to the scale-free nature of real-world networks. Such lower bound helps evaluate the performance of heuristic algorithms and thus reduce the uncertainty of the optimal solution of the network dismantling problem.},
author = {Sun, Jiachen and Liu, Rong and Fan, Zhengping and Xie, Jiarong and Ma, Xiao and Hu, Yanqing},
doi = {10.1063/1.5024338},
file = {:home/nahuel/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Sun et al. - 2018 - Lower bound of network dismantling problem.pdf:pdf},
issn = {10541500},
journal = {Chaos},
number = {6},
title = {{Lower bound of network dismantling problem}},
volume = {28},
year = {2018}
}
