issn = {1539-3755},
journal = {Physical Review E},
month = {apr},
number = {4},
pages = {042815},
title = {{Degree-dependent network growth: From preferential attachment to explosive percolation}},
url = {https://link.aps.org/doi/10.1103/PhysRevE.89.042815},
volume = {89},
year = {2014}
}
