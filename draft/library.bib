@article{DeMenezes2000,
abstract = {The small-world transition is a first-order transition at zero density p of shortcuts, whereby the normalised shortest-path distance ℒ = ℓ̄/L undergoes a discontinuity in the thermodynamic limit. On finite systems the apparent transition is shifted by $\Delta$p ∼ L-d. Equivalently a "persistence size" L* ∼ p-1/d can be defined in connection with finite-size effects. Assuming L* ∼ p-T, simple rescaling arguments imply that T = 1/d. We confirm this result by extensive numerical simulation in one to four dimensions, and argue that T = 1/d implies that this transition is first order.},
