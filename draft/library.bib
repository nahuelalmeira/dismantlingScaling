@article{Avetisov2018,
abstract = {We consider the clustering in rewired Erd˝os-R´enyi networks with conserved vertex degree and in reg- ular graphs from the localization perspective. It has been found in [1] that at some critical value µcr, where µ is the chemical potential µ of each closed triple of bonds, evolving networks decays into the maximally possible number of almost full subgraphs, and the spectrum of the adjacency matrix acquires the two-zonal support separated by a wide gap. Studying the level spacing distribution, we demonstrate that in one zone the distribution of gaps between neighboring eigenvalues shares Wigner surmise (i.e. is delocalized), however follows Poisson statistics in the second zone (i.e. is localized). In parallel with evolutionary designed networks, we consider ”instantly” ad hoc prepared networks with in- and cross-cluster probabilities as in the final stage of evolutionary network. For such ”instant” networks eigenvalues are delocalized in both zones. We speculate about the difference in eigenvalue statistics between ”dynamic” and ”static” networks from the perspective of a possible phase transition between ergodic and non-ergodic network patterns with a strong ”memory dependence”, thus advocating existence of non-ergodic delocalized states in the clustered random networks.},
archivePrefix = {arXiv},
arxivId = {arXiv:1303.5269v2},
author = {Avetisov, Vladik and Gorsky, Alexandr and Nechaev, Sergei and Valba, Olga},
