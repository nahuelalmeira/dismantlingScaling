issn = {20452322},
journal = {Scientific Reports},
number = {1},
pages = {1--13},
publisher = {Springer US},
title = {{Element-centric clustering comparison unifies overlaps and hierarchy}},
url = {http://dx.doi.org/10.1038/s41598-019-44892-y},
volume = {9},
year = {2019}
}
