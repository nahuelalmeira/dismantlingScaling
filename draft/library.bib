file = {:home/nahuel/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Clauset, Shalizi, Newman - 2009 - Power-Law Distributions in Empirical Data(2).pdf:pdf},
isbn = {9780769531762},
issn = {0036-1445},
journal = {SIAM Review},
keywords = {Power-law distribution,binned data,heavy-tailed distributions,model selection},
month = {nov},
number = {4},
pages = {661--703},
pmid = {271983500002},
title = {{Power-Law Distributions in Empirical Data}},
url = {http://epubs.siam.org/doi/abs/10.1137/070710111},
volume = {51},
